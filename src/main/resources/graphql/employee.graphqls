type Query {
    employees: [Employee]
}

type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput!): Employee!
}

type Employee {
    id: ID!
    name: String!
    salary: Float!
    email: String
    birthday: String!
    city: String!
    gender: String
}

input CreateEmployeeInput {
    name: String!
    salary: Float!
    email: String
    birthday: String!
    city: String!
    gender: String
}